{
  "name": "semverint",
  "version": "2.0.0",
  "description": "A TypeScript utility for converting semantic versions into integer representations.",
  "keywords": [
    "semver",
    "semantic version",
    "semantic version int",
    "semantic version integer",
    "semverint",
    "semver int",
    "semver integer"
  ],
  "license": "MIT",
  "type": "module",
  "packageManager": "npm@22.9.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brenank/semverint.git"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:esm": "tsc",
    "build": "npm run build:cjs && npm run build:esm",
    "validate": "npm run lint && npm run fmt && npm run test",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "fmt": "prettier --check .",
    "fmt:fix": "prettier --write .",
    "prepare": "husky",
    "checkchange": "beachball check",
    "change": "beachball change",
    "release": "beachball publish"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^22.8.1",
    "@types/semver": "^7.5.8",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "beachball": "2.47.1",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "3.3.3",
    "typescript": "^5.0.0",
    "vitest": "^2.1.3"
  }
}
